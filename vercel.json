{
	"rewrites": [
		{
			"source": "/",
			"destination": "/api/index"
		}
	],
	"functions": {
		"api/index.ts": {
			"maxDuration": 10,
			"includeFiles": "index.html"
		},
		"api/interactions.ts": {
			"maxDuration": 10,
			"includeFiles": "{src/commands/**,src/components/**,src/utils/**}"
		},
		"api/discord-oauth-callback.ts": {
			"maxDuration": 10,
			"includeFiles": "{dist/**,public/pages/**}"
		}
	}
}